   "source": [
    "a = int(input('Введіть число: '))\n",
    "\n",
    "k = 0 # лічильник для підрахунку кількості дільників\n",
    "\n",
    "for i in range(1, a + 1):\n",
    "    if a % i == 0:\n",
    "        k = k +1\n",
    "if k > 2: \n",
    "    print(\"Число \" + str(a) + \" є складене\")\n",
    "else:\n",
    "    print(\"Число \" + str(a) + \" є просте\")"
